{
  "compilerOptions": {
    "allowJs": true,
    "target": "esnext",
    "module": "esnext",
    "strict": true,
    "jsx": "preserve",
    "importHelpers": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "preserveSymlinks": true,
    "types": [
      "node",
      "webpack-env"
    ],
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ],
    "paths": {
      "@shell/components/*": [
        "../../node_modules/@rancher/shell/components/*"
      ],
      "@shell/core/*": [
        "../../node_modules/@rancher/shell/core/*"
      ],
      "@shell/config/*": [
        "../../node_modules/@rancher/shell/config/*"
      ],
      "@shell/store/*": [
        "../../node_modules/@rancher/shell/store/*"
      ],
      "@shell/plugins/*": [
        "../../node_modules/@rancher/shell/plugins/*"
      ],
      "@shell/utils/*": [
        "../../node_modules/@rancher/shell/utils/*"
      ],
      "@shell/models/*": [
        "../../node_modules/@rancher/shell/models/*"
      ],
      "@shell/mixins/*": [
        "../../node_modules/@rancher/shell/mixins/*"
      ],
      "@pkg/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.d.ts",
    "**/*.tsx",
    "**/*.vue",
    "../../node_modules/@rancher/shell/types/*.d.ts",
    "../../node_modules/@rancher/shell/core/**/*.ts",
    "../../node_modules/@rancher/shell/config/**/*.js",
    "../../node_modules/@rancher/shell/components/**/*.vue",
    "../../node_modules/@rancher/shell/plugins/**/*.js",
    "../../node_modules/@rancher/shell/utils/**/*.js",
    "../../node_modules/@rancher/shell/models/**/*.js",
    "../../node_modules/@rancher/shell/mixins/**/*",
    "../../node_modules/@rancher/shell/store/**/*.js",
    "../../node_modules/@rancher/components/**/*.vue",
    "../../node_modules/@rancher/components/**/*.ts",
    // "**/*.yaml",
    "../../node_modules/@rancher/shell/vue-shim.d.ts",
  ],
  "exclude": [
    "node_modules",
    "../../node_modules/@rancher/shell/**/*test.ts",
    "../../node_modules/@rancher/shell/**/*spec.ts",
    "../../node_modules/@rancher/components/**/*test.ts",
    "../../node_modules/@rancher/components/**/*spec.ts",
  ]
}